{
  "extends": ["eslint:recommended", "plugin:react/recommended", "next", "prettier"],
  "plugins": ["prettier"],
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "rules": {
    "prettier/prettier": "error",
    "react/jsx-sort-props": ["error", {
      "ignoreCase": true,
      "reservedFirst": true
    }]
  }
}
